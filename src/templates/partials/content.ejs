<% for (var i = 0; i < page.content.length; i += 1) { %>
    <% for (var j in page.content[i]) { %>
        <% if (j !== 'a') { %>
            <<%= j %>>
                <% if (typeof page.content[i].a !== 'undefined') { %>
                    <a href="<%= page.content[i].a%>">
                        <%= page.content[i][j] %>
                    </a>
                <% } else {%>
                    <%= page.content[i][j] %>
                <% } %>
            </<%= j %>>
        <% } %>
    <% } %>
<% } %>